{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./vercel/output/assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/vercel/output/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
